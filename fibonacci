#include <iostream>
using namespace std;

int main() {
    int count=0;
   int num,x = 0, y = 1, z = 0; count++;
   cout << "Enter the number : "; count++;
   cin >> num; count++;
    cout << "\nThe fibonacci series : " ; 
    count++;
    for (int i = 0; i < num; i++) {
      cout << x << " "<<endl;
     
      count++;
      z = x + y; count++;
      x = y; count++;
      y = z; count ++;
   }
   count++;
   cout<<count;
   return 0;
}